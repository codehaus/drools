<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<import resource="rulebase.appctx.xml" />

	<bean id="workingMemoryWithNonStandardName" lazy-init="true"
		class="org.drools.spring.factory.WorkingMemoryFactoryBean">
		<property name="ruleBase">
			<ref bean="ruleBase"/>
		</property>
	</bean>

	<bean id="workingMemoryListenerProcessor"
		class="org.drools.spring.event.WorkingMemoryEventListenerProcessor">
		<property name="workingMemory">
			<ref local="workingMemoryWithNonStandardName"/>
		</property>
	</bean>

	<bean id="listener_A" class="org.drools.spring.event.WorkingMemoryEventListenerProcessorTestCase$Listener_A" />
	<bean id="listener_B" class="org.drools.spring.event.WorkingMemoryEventListenerProcessorTestCase$Listener_B" />

</beans>